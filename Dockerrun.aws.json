{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "frontend",
      "image": "ahmedhawas/frontend",
      "hostname": "client",
      "essenial": false,
      "memory": 128
    },
    {
      "name": "server",
      "image": "ahmedhawas/server",
      "hostname": "api",
      "essenial": false,
      "memory": 128
    },
    {
      "name": "worker",
      "image": "ahmedhawas/worker",
      "essenial": false,
      "memory": 128
    },
    {
      "name": "nginx",
      "image": "ahmedhawas/nginx",
      "essenial": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["frontend", "server"],
      "memory": 128
    }
  ]
}
